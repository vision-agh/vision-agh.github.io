  additional_info = {. \textasteriskcenteredMore Information\textasteriskcentered can be [found here](https://github.com/alshedivat/al-folio/)},
  annotation    = {\textasteriskcentered Example use of superscripts<br>\textdagger{} Albert Einstein},
  selected      = {true},
